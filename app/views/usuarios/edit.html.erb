<h1>Editing usuario</h1>

<div class="row ">
  <div class="span6 offset3">
    <%= form_for (@usuario) do |f| %>
        <% render 'shared/error_messages' %>
        <%=  f.label :name, "Nombre" %>
        <%=  f.text_field :name %>
        <%=  f.label :empresa, "Empresa" %>
        <%=  f.text_field :empresa %>
        <%=  f.label :email %>
        <%=  f.text_field :email %>
        <%= f.label :password %>
        <%=  f.password_field :password %>
        <%= f.label :password_confirmation, "Confirmar Password" %>
        <%= f.password_field :password_confirmation %>
        <%= f.label :updated_at, "ActualizaciÃ³n" %>
        <%= f.datetime_select :updated_at %>

    <%= f.submit "Guardar",class: "btn btn-large btn-primary" %>  <% end %>



  </div>


</div>


