<h1>Dispositivos</h1>

<table  class="table .table-condensed ">
  <tr>
    <th>Usuario</th>
    <th>Creado por</th>
    <th>Status</th>
    <th>Device</th>
    <th>Sede</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dispositivos.each do |dispositivo| %>
  <tr>
    <td><%= dispositivo.usuario_id %></td>
    <td><%= dispositivo.creado_por %></td>
    <td><%= dispositivo.status %></td>
    <td><%= link_to dispositivo.device_id, dispositivo %></td>
    <td><%= dispositivo.sede_id %></td>
    <td><%= link_to 'Mostar', dispositivo %></td>
    <td><%= link_to 'Editar' %></td>
    <td><%= link_to 'Borrar', dispositivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dispositivo', new_dispositivo_path %>
